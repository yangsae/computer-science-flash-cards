[Unit]
Description=uWSGI instance to serve flash_cards
After=network.target

[Service]
User=yangsaerohoon

WorkingDirectory=/Users/yangsaerohoon/workspace2/computer-science-flash-cards
Environment="PATH=/Users/yangsaerohoon/workspace2/computer-science-flash-cards/flashcardenv/bin"
Environment="CARDS_SETTINGS=/Users/yangsaerohoon/workspace2/computer-science-flash-cards/config-personal.txt"
ExecStart=/Users/yangsaerohoon/workspace2/computer-science-flash-cards/flashcardenv/bin/uwsgi --ini flash_cards.ini

[Install]
WantedBy=multi-user.target